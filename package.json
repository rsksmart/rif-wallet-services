{
  "name": "rif-wallet-services",
  "version": "0.1.0",
  "description": "RIF Wallet Services",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon",
    "start:prod": "node lib/index.js",
    "demo": "nodemon --demo",
    "ganache": "npx ganache-cli",
    "test": "npx jest test/*/* --verbose --coverage --runInBand",
    "test:ci": "npx jest test/*/* --coverage --ci --runInBand",
    "test:watch": "npx jest test/*/* --verbose --coverage --watch --runInBand",
    "build": "rimraf ./lib && npx tsc",
    "build:prod": "rimraf ./lib && tsc -p tsconfig.json",
    "lint": "npx eslint ./src ./test --ext .json,.js,.ts",
    "lint:fix": "npx eslint ./src ./test --ext .json,.js,.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsksmart/rif-wallet-services.git"
  },
  "keywords": [
    "wallet",
    "services"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rsksmart/rif-wallet-services/issues"
  },
  "homepage": "https://github.com/rsksmart/rif-wallet-services#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "tsc": "^2.0.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@rsksmart/rsk-contract-metadata": "^1.0.15",
    "@rsksmart/rsk-utils": "^1.1.0",
    "axios": "^0.22.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  }
}
